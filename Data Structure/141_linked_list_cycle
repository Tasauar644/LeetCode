from typing import List

class ListNode:
    def __init__(self, x):
        self.val = x
        self.next = Noneslow, fast = head, head
         
        while fast and fast.next:
            slow = slow.next
            fast = fast,next.next
            if slow == fast:
                return True
        return False

class Solution:
     def hasCycle(self, head: Optional[ListNode]) -> bool:
        slow, fast = head, head
         
        while fast and fast.next:
            slow = slow.next
            fast = fast.next.next
            if slow == fast:
                return True
        return False         


S = Solution()
print(S.hasCycle([3,2,0,-4]))
